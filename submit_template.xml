<?xml version="1.0" encoding="utf-8" ?>

<job maxFilesPerProcess="10" fileListSyntax="xrootd" simulateSubmission="false">

  <command>
    starver SL20d
    <!--cat $FILELIST | grep -v "root://" > mynewile.list	-->
    <!--setenv FILELIST mynewile.list-->
		root4star -q -b -l RunAnalyzer.C+
		root4star -q -b -l PicoAnalyzer.cxx+\(\"$FILELIST\",\"$JOBID\",1,&cutID;,&varID;,&iterID;\) &gt;&amp; ${JOBID}.log
  </command>

  <SandBox installer="ZIP">
    <Package>
      <File>file:./RunAnalyzer.C</File>
      <File>file:./PicoAnalyzer.cxx</File>
      <File>file:./PicoAnalyzer.h</File>
      <File>file:./StRoot</File>
      <File>file:./EpCorrection_INPUT_sys_primary_var0_iter2_.root</File>
    </Package>
  </SandBox>


  <input URL="filelist:/star/data01/pwg/dchen/Ana/7p2GeV_FXT_2018_psi2/check/EpdAna/&listOfFiles;" nFiles="all" />

  <!-- <input URL="filelist:/star/u/fuchuan/3_85FXT/Analysis/v0Tree_Proton_Lm/3p85_local_goodrun.list" nFiles="all" /> -->
   
   <!-- <input URL="catalog:star.bnl.gov?production=P19ie,filetype=daq_reco_picoDst,trgsetupname=production_26p5GeV_fixedTarget_2018,library=SL20d,tpx=1,filename~st_physics,sanity=1,storage!=hpss" nFiles="all"/> -->

  <stdout discard="true" />
  <stderr URL="file:./err/$JOBID.err"/>

  <Generator>
	  <Location>./report/</Location>
	  <ScriptLocation>./csh/</ScriptLocation>
	  <ListLocation>./list/</ListLocation>
  </Generator>

  <output fromScratch="$JOBID.root" toURL="file:/star/data01/pwg/dchen/Ana/7p2GeV_FXT_2018_psi2/check/EpdAna/production/" />
  <output fromScratch="$JOBID.log" toURL="file:/star/data01/pwg/dchen/Ana/7p2GeV_FXT_2018_psi2/check/EpdAna/log/" />

</job>

